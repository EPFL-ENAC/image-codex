    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "generate:model": "rm -rf src/backend && openapi-generator-cli generate -i ../openapi.json -g typescript-axios -o src/backend",
