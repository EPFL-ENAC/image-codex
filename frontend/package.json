    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.0",
    "yaml-loader": "^0.8.0"
  },
  "overrides": {
    "dot-object": ">=2.1.3",
    "glob-parent": ">=6.0.1"
