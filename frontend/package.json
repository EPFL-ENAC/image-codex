    "downloadjs": "^1.4.7",
    "leaflet": "^1.7.1",
    "mime-types": "^2.1.33",
    "piexifjs": "^1.0.6",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-draggable-resizable": "^2.3.0",
    "vue-i18n": "^8.26.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vue2-leaflet": "^2.7.1",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.4.12",
